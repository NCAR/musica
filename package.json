{
  "name": "musica-addon",
  "version": "0.12.2",
  "description": "Native Node.js addon for MUSICA atmospheric chemistry modeling",
  "scripts": {
    "build": "cmake-js -p 4 compile --CDMUSICA_ENABLE_JAVASCRIPT=ON --CDMUSICA_ENABLE_PYTHON_LIBRARY=OFF --CDMUSICA_ENABLE_TUVX=OFF --CDMUSICA_ENABLE_CARMA=OFF --CDMUSICA_ENABLE_TESTS=OFF",
    "rebuild": "cmake-js rebuild",
    "clean": "cmake-js clean",
    "test": "node javascript/test/analytical.test.js",
    "sync-version": "node scripts/version_utils.js sync",
    "check-version": "node scripts/version_utils.js test"
  },
  "dependencies": {
    "musica": "^1.0.0",
    "node-addon-api": "^8.5.0"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0"
  },
  "cmake-js": {
    "runtime": "node",
    "runtimeVersion": "20.0.0"
  }
}
